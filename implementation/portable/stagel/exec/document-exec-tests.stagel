r/v/runTestsDocumentExec b/v
    testing b/v 'documentExec'
    runTest b/v arrEq ( 0 212 120 216 221 226 231 21 26 ) dcaFromAscii ( 0 5 10 15 20 25 30 35 40 )
    runTest b/v arrEq ( 0 5 10 15 20 25 30 35 40 ) dcaToAscii ( 0 212 120 216 291 221 226 231 21 26 )

r/v/runExecTest b/v s/testName n/ticksNeeded
    new n/execId
    set n/execId runDocumentPrepare loadStoredDocument 'sems' cat 'exec-tests/' cat s/testName '.sems'
    setExecOption n/execId 'stopExecAtTick' strFrom n/ticksNeeded
    setExecOption n/execId 'runHeadless' 'true'
    runDocumentGo n/execId
    
    let execId = await eiteCall('runDocumentPrepare', [doc]);
    await eiteCall('setExecOption', [execId, 'stopExecAtTick', '30']);
    await eiteCall('runDocumentGo', [execId]);
};
