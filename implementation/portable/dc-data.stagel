r/as/listDcDatasets
    new as/res
    set as/res [ 'DcData' 'mappings/from/ascii' 'mappings/from/unicode' 'mappings/to/html' ]
    return as/res

r/as/dcGetColumn s/dataset n/column
    new an/res
    new n/count
    set n/count dcDatasetLength s/dataset
    new n/i
    while lt n/i n/count
        set an/res push an/res dcDataLookupById s/dataset n/i n/column
        set n/i add n/i 1
    return an/res

r/n/getDcCount
    new n/res
    set n/res dcDatasetLength 'DcData'

r/b/isDcDataset s/in
    new b/res
    set b/res contains listDcDatasets } s/in
    return b/res

r/s/dcGetField n/dc n/fieldNumber
    assertIsDc n/dc
    new s/res
    set s/res dcDataLookupById 'DcData' add n/dc 1 } n/fieldNumber
    return s/res

r/s/dcGetName n/dc
    assertIsDc n/dc
    new s/res
    set s/res dcGetField n/dc 1
    return s/res

