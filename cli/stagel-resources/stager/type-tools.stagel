r/b/isByte n/in
    new b/res
    set b/res intIsBetween n/in 0 255
    return b/res

r/b/isChar g/in
    if not isStr g/in
        return false
    new s/val
    set s/val strFrom g/in
    new b/res
    set b/res isCharByte byteFromChar s/val
    return b/res

r/b/isCharByte n/in
    # Bear in mind that StageL doesn't attempt to support Unicode.
    new b/res
    set b/res intIsBetween n/in 32 126
    return b/res
