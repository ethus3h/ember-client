r/s/strCharAtPos s/str n/index
    new s/temp
    set s/temp substring s/str n/index 1
    return s/temp

r/s/strApp s/str1 s/str2
    new s/res
    set s/res cat s/str1 s/str2

r/s/reverseStr s/str
    new n/l
    set n/l len s/str
    new n/c
    set n/c 0
    new s/res
    while le n/c n/l
        set s/res cat s/res strCharAtPos s/str sub n/l n/c
    return s/res
