#!/bin/bash
dir="$1"
cmd="$2"
if [[ "" == "$cmd" ]]; then
    cmd="mkdir -p"
fi
cd "$dir" || die
i=0
gencmd="../gen"
[[ -e "./gen" ]] || gencmd="../../gen"
#a=$'150'; mkdir "$a"
while [[ "$i" -lt 500 ]]; do
    bash -c 'a=$'\'\\"$i"\''; xxd <<< "$a"; '"$cmd"' "$a"; cd "$a"; pwd; #'"$gencmd"' "$(pwd)" "touch"'
    i=$(( i + 1 ))
done